---
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import "../styles/global.css";
---

<html><head><meta charset="utf-8" /></head></html>
<Header />
<main class="mt-32 px-4 max-w-3xl mx-auto space-y-6">
  <h1 class="text-3xl font-bold mb-4">German C1 Certificate, December 2021</h1>
  <img src="/images/certificates/german/german-C1-2.jpg" alt="German C1 Certificate" style="max-width: 100%; height: auto;" />
  <p></p>
  <h1 class="text-3xl font-bold mb-4">German B2 Certificate, May 2021</h1>
  <img src="/images/certificates/german/german-B2-page1.jpg" alt="German B2 Certificate" style="max-width: 100%; height: auto;" />
  <img src="/images/certificates/german/german-B2-page2.jpg" alt="German B2 Certificate" style="max-width: 100%; height: auto;" />
  <p></p>
</main>
<Footer />
